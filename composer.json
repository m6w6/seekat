{
    "name": "m6w6/seekat",
    "description": "seekat wraps github api in php",
    "keywords": ["seekat", "github api", "api", "php"],
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "Michael Wallner",
            "email": "mike@php.net"
        }
    ],
    "autoload": {
        "psr-4": {
            "seekat\\": "lib/"
        },
        "files": [
            "lib/functions.php",
            "lib/API/functions.php",
            "lib/API/Future/functions.php",
            "lib/API/Links/functions.php",
            "lib/Exception/functions.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.1",
        "ext-http": ">=4.2",
        "ext-json": "*",
        "psr/log": "^3",
        "psr/cache": "^1",
        "psr/simple-cache": "^1",
        "rize/uri-template": "^0.3.4",
        "monolog/monolog": "^3"
    },
    "require-dev": {
        "php": ">=8.1",
        "amphp/amp": "^2",
        "react/promise": "^2",
        "phpunit/phpunit": "^9",
        "squizlabs/php_codesniffer": "*",
        "phpcompatibility/php-compatibility": "*"
    }
}
